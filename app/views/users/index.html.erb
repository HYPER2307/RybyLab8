<%= stylesheet_link_tag "index", "data-turbo-track": "reload" %>
<p style="color: green"><%= notice %></p>
<div class="content">
  <h1>Users</h1>
  <div id="users">
    <% @users.each do |user| %>
      <%= render user %>
      <p>
        <%= link_to "Show this user", user %>
      </p>
    <% end %>
  </div>
  <a href="http://127.0.0.1:3000/users/new">
    <button type="button" class="btn btn-warning"> New users</button>
  </a>
</div>
